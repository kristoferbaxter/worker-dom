{
  "compileOnSave": true,
  "compilerOptions": {
    "lib": [
      "es2017",
      "webworker"
    ],
    "outDir": "../output",
    "sourceMap": true,
    "moduleResolution": "node",
    "target": "esnext",
    "baseUrl": "../",
    "allowJs": false,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "paths": {
      "@ampproject/worker-dom-transport/*": ["worker-dom-transport/src/*"],
    }
  },
  "include": [
    "./**/*.ts"
  ]
}